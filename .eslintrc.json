{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-trailing-spaces":"off",
        "comma-dangle":"off",
        "eol-last":"off",
        "comma-spacing":"off",
        "no-console":"off",
        "no-underscore-dangle":"off",
        "no-empty-function":"off",
        "no-unused-vars":"off",
        "no-restricted-globals":"off",
        "no-undef":"off"
    }
}
